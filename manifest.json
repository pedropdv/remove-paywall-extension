{
  "manifest_version": 3,
  "name": "Remove Paywall",
  "version": "1.0",
  "description": "Redirects current page to https://removepaywall.com",
  "author": "Pedro de Vasconcellos",
  "homepage_url": "https://github.com/pedropdv/remove-paywall-extension",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_title": "Remover Paywall",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
